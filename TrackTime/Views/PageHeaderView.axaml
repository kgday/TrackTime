<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:views="using:TrackTime.Views"
             mc:Ignorable="d" 
             x:Class="TrackTime.Views.PageHeaderView"
             Padding="5">
  <StackPanel Orientation="Horizontal">
    <Image x:Name="Image" Width="24" Height="24" Margin="0,0,5,0" Source="{StaticResource Home}"/>
    <TextBlock x:Name="TitleText" Text="Home" VerticalAlignment="Center"/>
  </StackPanel>
</UserControl>
